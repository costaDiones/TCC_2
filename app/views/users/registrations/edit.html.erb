  <h2>Edit <%= resource_name.to_s.humanize %></h2>

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
    <%= devise_error_messages! %>

    <div id="wrapper">

      <!-- Navigation -->
      <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/users/sign_in">CAOSC</a>
        </div>
        <!-- Top Menu Items -->
        <ul class="nav navbar-right top-nav">


          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> <%= current_user.nome %><b class="caret"></b></a>
            <ul class="dropdown-menu">
              <li>
                <a href="editar"><i class="fa fa-fw fa-pencil"></i> Perfil</a>
              </li>


              <li>
                <a href="mensagem"><i class="fa fa-fw fa-envelope" ></i> Mensagem</a>
              </li>

              <li class="divider"></li>
              <li>
              <%= link_to('Sair', destroy_user_session_path, :method => :delete) %>
              </li>
            </ul>
          </li>
        </ul>
        <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
        <div class="collapse navbar-collapse navbar-ex1-collapse">
          <ul class="nav navbar-nav side-nav">


            <li >
              <a href="/consumo"><i class="fa fa-fw fa-dashboard"></i> Consumo</a>
            </li>


            <li class="footerFinal">
              <a href="javascript:;" data-toggle="collapse" data-target="#demo"><i class="fa fa-fw fa-arrows-v"></i> Chamados <i class="fa fa-fw fa-caret-down"></i></a>
              <ul id="demo" class="collapse ">
                <li >
                  <a class="footerFinal" href="/novochamado">Novo Chamado</a>
                </li>
                <li >
                  <a class="footerFinal" href="/chamados">Meus Chamados</a>
                </li>
              </ul>
            </li>
            <li >
              <a href="/tabelaPrecos"><i class="fa fa-fw fa-money"></i> Tabela de Preços</a>
            </li>

            <li >
              <a href="/usuarios"><i class="fa fa-fw fa-user"></i> Administrador</a>
            </li>

            <li class="active">
              <a href=""><i class="fa fa-fw fa-edit"></i> Perfil</a>
            </li>


          </ul>
        </div>
        <!-- /.navbar-collapse -->
      </nav>

      <div id="page-wrapper">

        <div class="container-fluid">

          <!-- Page Heading -->
          <div class="container">

            <div class="col-lg-12">

              <form role="form">


                <div class="form-group  col-sm-6 ">
                <%= label_tag(:datanascimento, "Nome Completo ") %>
                  <%= f.text_field :nome, autofocus: true, class:'form-control', placeholder:'Nome'%>


                </div>
                <div class="form-group  col-sm-6  ">
                  <%= label_tag(:datanascimento, "Data Nascimento:  ") %>
                  <%= f.date_field :datanascimento, autofocus: true, class:'form-control', placeholder:'Data Nascimento'%>

                </div>

                <div class="form-group  col-sm-6 ">
                    <%= label_tag(:sexo, "Sexo:  ") %>
                    <%= f.radio_button :sexo, 'Masculino' %>
                    <%= label_tag(:sexo, " Masculino ") %>
                    <%= f.radio_button :sexo, 'Feminino' %>
                    <%= label_tag("Feminino") %>

                  </div>

                <div class="form-group col-sm-6 ">
                  <%= f.text_field :cpf, autofocus: true, class:'form-control', placeholder:'CPF'%>

                </div>




                <div class="form-group  col-sm-6  ">
                  <%= f.text_field :endereco, autofocus: true, class:'form-control', placeholder:'Endereco'%>


                </div>

                <div class="form-group  col-sm-6 ">
                  <%= f.text_field :bairro, autofocus: true, class:'form-control', placeholder:'Bairro'%>

                </div>

                <div class="form-group col-sm-1 ">
                  <%= f.text_field :numero, autofocus: true, class:'form-control', placeholder:'Número'%>


                </div>

                <div class="form-group col-sm-3  ">
                  <%= f.text_field :cidade, autofocus: true, class:'form-control', placeholder:'Cidade'%>


                </div>


                <div class="form-group  col-sm-2  ">
                <%= f.text_field :uf, autofocus: true, class:'form-control', placeholder:'Estado'%>


                </div>

                <div class="form-group  col-sm-3  ">
                <%= f.text_field :complemento, autofocus: true, class:'form-control', placeholder:'Complemento'%>


                </div>

                <div class="form-group col-sm-3 ">
                  <%= f.text_field :telefone, autofocus: true, class:'form-control', placeholder:'Telefone'%>

                </div>

                <div class="form-group  col-sm-3 ">

                <%= f.email_field :email, autofocus: true, class:'form-control', :disabled => true %>
                </div>
                <div class="form-group col-sm-3 ">

                    <%= f.password_field :current_password, autocomplete: "off" ,class:'form-control', placeholder:'Senha Atual'%>
                  </div>

                <div class="form-group  col-sm-3 ">

                  <% if @minimum_password_length %>
                  <% end %>
                  <%= f.password_field :password, autocomplete: "off",class:'form-control', placeholder:'Nova Senha' %>
                </div>

                <div class="form-group  col-sm-3 ">
                  <%= f.password_field :password_confirmation, autocomplete: "off",class:'form-control', placeholder:'Confirma Senha' %>
                </div>






                <div class="form-group  col-xs-8 ">
                      <button type="submit" class="botaoEntra btn btn-primary">
                          Alterar
                        </button>

                </div>

              </form>

            <% end -%>
            <div class="form-group  col-sm-10 ">

                    <%= link_to "Excluir Conta",  registration_path(resource_name), data: { confirm: "Você tem certeza?" }, method: :delete %></p>


            </div>
            </div>

          </div>







          <!-- /.row -->

        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="js/plugins/morris/raphael.min.js"></script>
    <script src="js/plugins/morris/morris.min.js"></script>
    <script src="js/plugins/morris/morris-data.js"></script>
